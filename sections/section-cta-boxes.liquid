{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}
<div class="section-{{ section.id }}-padding tw-px-4">
  <div class="tw-mx-auto tw-max-w-screen-xl tw-border tw-border-steel tw-rounded-xl">
    <div class="tw-grid tw-grid-cols-2 md:tw-grid-cols-4 tw-divide-x tw-divide-y md:tw-divide-y-0 tw-divide-steel">
      <a
        class="tw-flex tw-flex-col tw-p-4 lg:tw-p-6 xl:tw-p-8 tb-quick-links-divide-fix hover:tw-bg-sheep/70 first:md:tw-rounded-l-xl last:md:tw-rounded-r-xl tw-group/item"
        href="/pages/roof-tent-guide"
        data-tracking="quick-links-Roof-Tent-Guide"
      >
        <div class="tw-h-10 tw-w-10 tw-mb-1 md:tw-mb-3 tw-text-tugboat">
          <svg
            xmlns="http://www.w3.org/2000/svg"
            viewBox="0 0 24 24"
            fill="none"
            stroke="currentColor"
            stroke-width="1.2"
            stroke-linecap="round"
            stroke-linejoin="round"
          >
            <path d="M19 20 10 4"></path><path d="m5 20 9-16"></path><path d="M3 20h18"></path><path d="m12 15-3 5"></path><path d="m12 15 3 5"></path>
