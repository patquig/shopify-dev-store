            fill="none"
            viewBox="0 0 24 24"
            stroke-width="1.2"
            stroke="currentColor"
          >
            <path stroke-linecap="round" stroke-linejoin="round" d="M2.25 12.76c0 1.6 1.123 2.994 2.707 3.227 1.087.16 2.185.283 3.293.369V21l4.076-4.076a1.526 1.526 0 011.037-.443 48.282 48.282 0 005.68-.494c1.584-.233 2.707-1.626 2.707-3.228V6.741c0-1.602-1.123-2.995-2.707-3.228A48.394 48.394 0 0012 3c-2.392 0-4.744.175-7.043.513C3.373 3.746 2.25 5.14 2.25 6.741v6.018z"></path>
          </svg>
        </div>
        <h4 class="tw-text-base md:tw-text-lg tw-font-body tw-normal-case tw-font-medium tw-mb-1.5 md:tw-mb-0">
          Contact Us
        </h4>
        <p class="tw-text-battleship md:tw-text-left tw-text-sm lg:tw-text-base tw-mb-1.5 md:tw-mb-3 tw-hidden md:tw-block">
          Our team is ready to help
        </p>
        <div class="tw-flex tw-items-center group-hover/item:tw-underline tw-underline-offset-4 tw-text-orange tw-font-medium tw-text-sm sm:tw-text-base">
          <div>Chat now</div>
          <div class="tw-h-3 tw-w-3 tw-ml-0.75 sm:tw-h-3.5 sm:tw-w-3.5 sm:tw-ml-1">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke-width="1.5"
              stroke="currentColor"
            >
              <path stroke-linecap="round" stroke-linejoin="round" d="M13.5 4.5L21 12m0 0l-7.5 7.5M21 12H3"></path>
            </svg>
          </div>
        </div>
      </a>
    </div>
  </div>
</div>

{% schema %}
{
  "name": "CTA boxes",
  "tag": "section",
  "settings": [
    {
      "type": "header",
      "content": "Padding"
    },
    {
      "type": "range",
      "id": "padding_top",
      "label": "Padding Top",
      "default": 0,
      "min": 0,
      "max": 100,
      "step": 10
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "label": "Padding Bottom",
      "default": 0,
      "min": 0,
      "max": 100,
      "step": 10
    }
  ],
  "presets": [
    {
      "name": "CTA boxes",
      "settings": {}
    }
  ]
}
{% endschema %}
